<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".registrar_usuario"
    android:background="#EEF9ED">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.constraint.Guideline
            android:id="@+id/guideline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="581dp"
            android:layout_marginEnd="411dp"
            android:layout_marginBottom="150dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintGuide_begin="562dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <android.support.constraint.Guideline
            android:id="@+id/guideline5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="94dp"
            android:layout_marginEnd="411dp"
            android:layout_marginBottom="637dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintGuide_begin="94dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="429dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.38">

            <android.support.v7.widget.CardView
                android:layout_width="376dp"
                android:layout_height="403dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:background="#EEF9ED"
                app:cardBackgroundColor="#EEF9ED">

                <android.support.constraint.ConstraintLayout
                    android:layout_width="390dp"
                    android:layout_height="405dp"
                    android:background="#EEF9ED">

                    <LinearLayout
                        android:layout_width="370dp"
                        android:layout_height="391dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="6dp"
                        android:layout_marginBottom="4dp"
                        android:background="#EEF9ED"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <android.support.constraint.ConstraintLayout
                            android:layout_width="339dp"
                            android:layout_height="match_parent">

                            <EditText
                                android:id="@+id/editText21"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="8dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="nombre"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toTopOf="@+id/editText22"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <EditText
                                android:id="@+id/editText22"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="8dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="apellido"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toTopOf="@+id/editText25"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/editText21" />

                            <EditText
                                android:id="@+id/editText25"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="8dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="pais"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toTopOf="@+id/editText23"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/editText22" />

                            <EditText
                                android:id="@+id/editText23"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="7dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="celular"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toTopOf="@+id/editText24"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/editText25" />

                            <EditText
                                android:id="@+id/editText26"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="3dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="descripción"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/editText24" />

                            <EditText
                                android:id="@+id/editText24"
                                android:layout_width="312dp"
                                android:layout_height="47dp"
                                android:layout_marginStart="8dp"
                                android:layout_marginTop="8dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="8dp"
                                android:backgroundTint="@color/colorPrimary"
                                android:ems="10"
                                android:hint="género"
                                android:inputType="textPersonName"
                                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                android:textColorHint="#9D9F9D"
                                android:textSize="18sp"
                                app:layout_constraintBottom_toTopOf="@+id/editText26"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/editText23" />
                        </android.support.constraint.ConstraintLayout>

                    </LinearLayout>

                </android.support.constraint.ConstraintLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="390dp"
            android:layout_height="72dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/guideline5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#EEF9ED">

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="298dp"
                        android:layout_height="46dp"
                        android:layout_marginStart="44dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="12dp"
                        android:text="Registrar información"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="30sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </android.support.constraint.ConstraintLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="383dp"
            android:layout_height="105dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline8">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:cardBackgroundColor="#EEF9ED">

                <android.support.constraint.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <Button
                        android:id="@+id/button6"
                        android:layout_width="200dp"
                        android:layout_height="47dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp"
                        android:background="@drawable/button_rounded"
                        android:onClick="onClick"
                        android:text="guardar"
                        android:textColor="@color/cardview_light_background"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </android.support.constraint.ConstraintLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>
    </android.support.constraint.ConstraintLayout>

</RelativeLayout>
